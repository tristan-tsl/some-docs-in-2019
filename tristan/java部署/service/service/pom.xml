        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.account.agent.basic</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.account.agent.basic-1.0.0.jar</systemPath>
        </dependency>
        <!--批量添加子账号授权-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.auth</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.auth-1.0.0.jar</systemPath>
        </dependency>
        <!--<dependency>-->
            <!--<groupId>com.alibaba</groupId>-->
            <!--<artifactId>ocean.client.java.auth.sources</artifactId>-->
            <!--<version>1.0.0</version>-->
            <!--<scope>system</scope>-->
            <!--<systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.auth.sources-1.0.0.jar</systemPath>-->
        <!--</dependency>-->
        <!--批量查询子账号授权-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.auth.list</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.auth.list-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.auth.list.sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.auth.list.sources-1.0.0.jar</systemPath>
        </dependency>
        <!--获取子账号列表-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.account.list</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.account.list-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.account.list.sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.account.list.sources-1.0.0.jar</systemPath>
        </dependency>
        <!--消息订阅-->
        <dependency>
            <groupId>com.alibaba.aop</groupId>
            <artifactId>aop-sdk-message</artifactId>
            <version>0.9.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/aop-sdk-message-0.9.0.jar</systemPath>
        </dependency>
        <!--okHttp请求-->
        <dependency>
            <groupId>com.squareup.okhttp3</groupId>
            <artifactId>okhttp</artifactId>
            <version>3.6.0</version>
        </dependency>

