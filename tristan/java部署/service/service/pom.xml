        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.create.order.preview</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.create.order.preview-1.0.0.jar
            </systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.create.order.preview-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.create.order.preview-sources-1.0.0.jar
            </systemPath>
        </dependency>
        <!--创建订单预览结束-->
        <!--获取订单详情-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.product.info</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.product.info-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.product.info-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.product.info-sources-1.0.0.jar
            </systemPath>
        </dependency>
        <!--获取订单详情结束-->
        <!--将商品铺货-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.product.pushed</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.product.pushed-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.product.pushed-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.product.pushed-sources-1.0.0.jar
            </systemPath>
        </dependency>
        <!--将商品铺货结束-->
