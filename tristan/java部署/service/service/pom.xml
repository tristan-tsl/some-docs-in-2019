        <!--采购订单确认收货-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>order-receiveGoods</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/order-receiveGoods-1.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>order-receiveGoods-biz-sources</artifactId>
            <version>1.0</version>
