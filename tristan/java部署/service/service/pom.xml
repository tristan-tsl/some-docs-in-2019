<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.ebuy.cloud.basics</groupId>
        <artifactId>cloud-basics</artifactId>
        <version>1.1-SNAPSHOT</version>
        <!--<relativePath/> &lt;!&ndash; lookup parent from repository &ndash;&gt;-->
    </parent>
    <groupId>com.ebuy.cloud.basic.alibaba.order</groupId>
    <artifactId>service-alibaba-order</artifactId>
    <name>service-alibaba-order</name>
    <description>service of alibaba order</description>

    <properties>
        <java.version>1.8</java.version>
        <cloud.server.start>com.ebuy.cloud.basic.alibaba.order.servicealibabaorder.ServiceAlibabaOrderApplication
        </cloud.server.start>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.springframework.cloud</groupId>
            <artifactId>spring-cloud-starter-openfeign</artifactId>
            <!--<exclusions>
                <exclusion>
                    <artifactId>HdrHistogram</artifactId>
                    <groupId>org.hdrhistogram</groupId>
                </exclusion>
            </exclusions>-->
        </dependency>
        <!-- 1688一键下单-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean-client</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean-client-1.0.Jar</systemPath>
        </dependency>
        <!-- 商品信息查询-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean-product</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean-product-1.0.jar</systemPath>
        </dependency>
        <!-- 跨境订单创建  -->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean-cross-order</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean-cross-order-1.0.jar</systemPath>
        </dependency>
        <!--1688买家查看获得所有账期授信-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.1.1</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/commons-logging-1.1.1.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>fastjson</artifactId>
            <version>1.2.48</version>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.biz</artifactId>
            <version>1.1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.biz-1.1.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.java.biz-sources</artifactId>
            <version>1.1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.java.biz-sources-1.1.0.jar</systemPath>
        </dependency>
        <!--结束1688买家查看获得所有账期授信-->
        <!--1688订单详情查看-买家视角-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.order</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.order-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.order-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.order-sources-1.0.0.jar</systemPath>
        </dependency>
        <!--结束1688订单详情查看-买家视角-->
        <!--获取跨境宝支付的支付链接-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.cross.border.pay</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.cross.border.pay-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.cross.border.pay-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.cross.border.pay-sources-1.0.0.jar
            </systemPath>
        </dependency>
        <!--结束获取跨境宝支付的支付链接-->
        <!--获取交易订单的物流信息-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.logistics.info</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.logistics.info-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.logistics.info-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.logistics.info-sources-1.0.0.jar
            </systemPath>
        </dependency>
        <!--结束获取交易订单的物流信息-->
        <!--物流追踪信息-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.logistics.tracking</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.logistics.tracking-1.0.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>ocean.client.logistics.tracking-sources</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/ocean.client.logistics.tracking-sources-1.0.0.jar
            </systemPath>
        </dependency>
        <!--结束物流追踪信息-->
        <dependency>
            <groupId>com.ebuy.cloud.commons</groupId>
            <artifactId>commons-http</artifactId>
            <version>${ebuy-cloud.version}</version>
            <scope>compile</scope>
        </dependency>

        <!--取消交易-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>trade-cancel</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/trade-cancel-1.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>trade-cancel-biz-sources</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/trade-cancel-biz-sources-1.0.jar</systemPath>
        </dependency>
