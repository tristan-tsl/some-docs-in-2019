        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>trade-receiveAddress</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/trade-receiveAddress-1.0.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>trade-receiveAddress-biz-sources</artifactId>
            <version>1.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/trade-receiveAddress-biz-sources-1.0.jar</systemPath>
        </dependency>
        <!--结束买家获取保存的收货地址信息列表-->
